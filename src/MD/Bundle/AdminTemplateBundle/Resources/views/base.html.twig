<!DOCTYPE html>
<html lang="en">
    <head>
        {% block meta %}

            <meta charset="utf-8">
            <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
            <meta name="description" content="Sentir, Responsive admin and dashboard UI kits template">
            <meta name="keywords" content="admin,bootstrap,template,responsive admin,dashboard template,web apps template">
            <meta name="author" content="Ari Rusmanto, Isoh Design Studio, Warung Themes">
            <link rel="shortcut icon" href="{{ asset('bundles/fe/css/assets/ico/favicon.ico') }}" type="image/x-icon">
        {% endblock %}
        <title>{{ page_title }} :: GT Egypt Admin Panel</title>

        {% block stylesheets %}
            <!-- BOOTSTRAP CSS (REQUIRED ALL PAGE)-->
            <link href="{{ asset('bundles/admintemplate/assets/css/bootstrap.min.css') }}" rel="stylesheet">

            <!-- PLUGINS CSS -->
            <link href="{{ asset('bundles/admintemplate/assets/plugins/owl-carousel/owl.carousel.min.css') }}" rel="stylesheet">
            <link href="{{ asset('bundles/admintemplate/assets/plugins/owl-carousel/owl.theme.min.css') }}" rel="stylesheet">
            <link href="{{ asset('bundles/admintemplate/assets/plugins/owl-carousel/owl.transitions.min.css') }}" rel="stylesheet">

            <!-- MAIN CSS (REQUIRED ALL PAGE)-->
            <link href="{{ asset('bundles/admintemplate/assets/plugins/font-awesome/css/font-awesome.min.css') }}" rel="stylesheet">
            <link href="{{ asset('bundles/admintemplate/assets/css/style.css') }}" rel="stylesheet">
            <link href="{{ asset('bundles/admintemplate/assets/css/style-responsive.css') }}" rel="stylesheet">

            <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
            <!--[if lt IE 9]>
            <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js') }}"></script>
            <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js') }}"></script>
            <![endif]-->
        {% endblock %}
    </head>
    <body class="tooltips">
        <!--
        ===========================================================
        BEGIN PAGE
        ===========================================================
        -->
        <div class="wrapper">
            <!-- BEGIN TOP NAV -->
            <div class="top-navbar">
                <div class="top-navbar-inner info-color">

                    <!-- Begin Logo brand -->
                    <div class="logo-brand info-color">
                        <a href="{{ path('dashboard') }}"><img src="{{ asset('bundles/admintemplate/assets/img/sentir-logo-primary.png') }}" alt="Sentir logo"></a>
                    </div><!-- /.logo-brand -->
                    <!-- End Logo brand -->

                    <div class="top-nav-content">

                        <!-- Begin button sidebar left toggle -->
                        <div class="btn-collapse-sidebar-left">
                            <i class="fa fa-long-arrow-right icon-dinamic"></i>
                        </div><!-- /.btn-collapse-sidebar-left -->
                        <!-- End button sidebar left toggle -->

                        <!-- Begin button nav toggle -->
                        <div class="btn-collapse-nav" data-toggle="collapse" data-target="#main-fixed-nav">
                            <i class="fa fa-plus icon-plus"></i>
                        </div><!-- /.btn-collapse-sidebar-right -->
                        <!-- End button nav toggle -->

                        <!-- Begin user session nav -->
                        <ul class="nav-user navbar-right">
                            <li class="dropdown">
                                <a href="#fakelink" class="dropdown-toggle" data-toggle="dropdown">
                                    Hi, <strong>{{ app.user.username|capitalize }}</strong>
                                </a>
                                <ul class="dropdown-menu square primary margin-list-rounded with-triangle">
                                    <li><a href="{{ url('logout') }}">Log out</a></li>
                                </ul>
                            </li>
                        </ul>
                        <!-- End user session nav -->
                    </div><!-- /.top-nav-content -->
                </div><!-- /.top-navbar-inner -->
            </div><!-- /.top-navbar -->
            <!-- END TOP NAV -->

            <!-- BEGIN SIDEBAR LEFT -->
            <div class="sidebar-left sidebar-nicescroller">
                <ul class="sidebar-menu">
                    {% include 'AdminTemplateBundle::menuCMS.html.twig' %}
                </ul>
            </div><!-- /.sidebar-left -->
            <!-- END SIDEBAR LEFT -->

            <!-- BEGIN PAGE CONTENT -->
            <div class="page-content">
                <div class="container-fluid">
                    <!-- Begin page heading -->
                    <h1 class="page-heading">{{ page_title }}</h1>
                    <!-- End page heading -->
                {% block body %}{%  endblock %}
            </div><!-- /.container-fluid -->
            <!-- BEGIN FOOTER -->
            <footer>
                &copy; 2014 GT Egypt<br />
                Powered by <a href="http://perfectneeds.com" target="_blank">PerfectNeeds</a>.
            </footer>
            <!-- END FOOTER -->
        </div><!-- /.page-content -->
    </div><!-- /.wrapper -->
    <!-- END PAGE CONTENT -->

    {% block javascripts %}
        <!--
        ===========================================================
        Placed at the end of the document so the pages load faster
        ===========================================================
        -->

        <!-- MAIN JAVASRCIPT (REQUIRED ALL PAGE)-->
        <script src="{{ asset('bundles/admintemplate/assets/js/jquery.min.js') }}"></script>
        <script src="{{ asset('bundles/admintemplate/assets/js/bootstrap.min.js') }}"></script>
        <script src="{{ asset('bundles/admintemplate/assets/plugins/retina/retina.min.js') }}"></script>
        <script src="{{ asset('bundles/admintemplate/assets/plugins/nicescroll/jquery.nicescroll.js') }}"></script>
        <script src="{{ asset('bundles/admintemplate/assets/plugins/slimscroll/jquery.slimscroll.min.js') }}"></script>
        <script src="{{ asset('bundles/admintemplate/assets/plugins/backstretch/jquery.backstretch.min.js') }}"></script>

        <!-- PLUGINS -->
        <script src="{{ asset('bundles/admintemplate/assets/plugins/prettify/prettify.js') }}"></script>
        <script src="{{ asset('bundles/admintemplate/assets/plugins/owl-carousel/owl.carousel.min.js') }}"></script>

    {% endblock %}
    <!-- MAIN APPS JS -->
    <script src="{{ asset('bundles/admintemplate/assets/js/apps.js') }}"></script>
</body>
</html>
