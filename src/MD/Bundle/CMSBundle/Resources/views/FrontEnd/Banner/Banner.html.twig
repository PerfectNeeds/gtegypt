{% if Banners %}
<section id="one-parallax" class="parallax" {#style="background-image: url('{{ asset('bundles/fe/images/demos/01_parallax.jpg') }}');"#} data-stellar-background-ratio="0.6" data-stellar-vertical-offset="20">
    <div class="mapandslider">
        <div class="overlay1">
            <div class="container">
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div id="property-slider" class="clearfix">
                            <div class="flexslider">
                                <ul class="slides">
                                {% for banner in Banners %}
                                    <li style="height: 393px;">
                                        <div class="banner_data">
                                            <h2>{{ banner.title }}</h2>
                                            <p>{{ banner.text }}</p>
                                            {% if banner.url != '#' and banner.url !='' %}
                                            <a href="{{ banner.url }}" {% if banner.openType== 1 %}target="_blank"{% endif %} style="color:#fff;background:#644a90;padding:4px 6px;">Learn more</a>
                                            {% endif %}
                                        </div>
                                        <a href="#" class="images-link"><img src="{{ asset('uploads/banners/'~banner.id~"/"~banner.image.id) }}" alt="{{ banner.title }}"></a>
                                    </li>
                                {% endfor %}
                                </ul><!-- end slides -->
                            </div><!-- end flexslider -->
                        </div><!-- end property-slider -->
                    </div><!-- end col-lg-8 -->
                </div><!-- end row -->
            </div><!-- end dm_container -->
        </div>
    </div>
</section><!-- end mapandslider -->
{% endif %}